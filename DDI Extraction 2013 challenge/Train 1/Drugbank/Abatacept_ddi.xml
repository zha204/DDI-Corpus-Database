<document id="DDI-DrugBank.d297">
    <sentence id="DDI-DrugBank.d297.s0" text="Formal drug interaction studies have not been conducted with ORENCIA.">
        <entity id="DDI-DrugBank.d297.s0.e0" charOffset="61-67"
            type="brand" text="ORENCIA"/>
    </sentence>
    <sentence id="DDI-DrugBank.d297.s1" text="Population pharmacokinetic analyses revealed that MTX, NSAIDs, corticosteroids, and TNF blocking agents did not influence abatacept clearance.">
        <entity id="DDI-DrugBank.d297.s1.e0" charOffset="50-52"
            type="drug" text="MTX"/>
