    </sentence>
    <sentence id="DDI-DrugBank.d297.s4" text="Concurrent therapy with ORENCIA and TNF antagonists is not recommended.">
        <entity id="DDI-DrugBank.d297.s4.e0" charOffset="24-30"
            type="brand" text="ORENCIA"/>
        <entity id="DDI-DrugBank.d297.s4.e1" charOffset="36-50"
