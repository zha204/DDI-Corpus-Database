<document id="DDI-DrugBank.d335">
    <sentence id="DDI-DrugBank.d335.s0" text="If TRANXENE is to be combined with other drugs acting on the central nervous system, careful consideration should be given to the pharmacology of the agents to be employed.">
        <entity id="DDI-DrugBank.d335.s0.e0" charOffset="3-10"
            type="brand" text="TRANXENE"/>
    </sentence>
    <sentence id="DDI-DrugBank.d335.s1" text="Animal experience indicates that clorazepate dipotassium prolongs the sleeping time after hexobarbital or after ethyl alcohol, increases the inhibitory effects of chlorpromazine, but does not exhibit monoamine oxidase inhibition.">
        <entity id="DDI-DrugBank.d335.s1.e0" charOffset="33-55"
            type="drug" text="clorazepate dipotassium"/>
        <entity id="DDI-DrugBank.d335.s1.e1" charOffset="90-101"
            type="drug" text="hexobarbital"/>
        <entity id="DDI-DrugBank.d335.s1.e2" charOffset="112-124"
            type="drug" text="ethyl alcohol"/>
        <entity id="DDI-DrugBank.d335.s1.e3" charOffset="163-176"
            type="drug" text="chlorpromazine"/>
        <ddi id="DDI-DrugBank.d335.s1.d0" e1="DDI-DrugBank.d335.s1.e0"
