        <entity id="DDI-DrugBank.d460.s11.e6" charOffset="143-150"
            type="drug" text="warfarin"/>
        <entity id="DDI-DrugBank.d460.s11.e7" charOffset="153-161"
            type="drug" text="phenytoin"/>
        <entity id="DDI-DrugBank.d460.s11.e8" charOffset="164-183"
            type="drug" text="acetylsalicylic acid"/>
        <entity id="DDI-DrugBank.d460.s11.e9" charOffset="186-196"
            type="drug" text="tolbutamide"/>
        <entity id="DDI-DrugBank.d460.s11.e10" charOffset="203-211"
            type="drug" text="metformin"/>
        <entity id="DDI-DrugBank.d460.s11.e11" charOffset="250-260"
            type="drug" text="nateglinide"/>
    </sentence>
    <sentence id="DDI-DrugBank.d460.s12" text="Similarly, nateglinide had no influence on the serum protein binding of propranolol, glyburide, nicardipine, warfarin, phenytoin, acetylsalicylic acid, and tolbutamide in vitro .">
