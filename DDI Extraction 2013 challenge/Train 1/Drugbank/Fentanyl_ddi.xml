            type="drug" text="ketoconazole"/>
        <entity id="DDI-DrugBank.d170.s2.e3" charOffset="112-123"
            type="drug" text="itraconazole"/>
        <entity id="DDI-DrugBank.d170.s2.e4" charOffset="126-139"
            type="drug" text="troleandomycin"/>
        <entity id="DDI-DrugBank.d170.s2.e5" charOffset="142-155"
            type="drug" text="clarithromycin"/>
