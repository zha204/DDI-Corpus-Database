            type="group" text="beta-adrenergic receptor blockers"/>
        <entity id="DDI-DrugBank.d532.s2.e3" charOffset="86-112"
            type="group" text="calcium channel antagonists"/>
        <entity id="DDI-DrugBank.d532.s2.e4" charOffset="115-154"
            type="group" text="angiotensin converting enzyme inhibitors"/>
        <entity id="DDI-DrugBank.d532.s2.e5" charOffset="178-185"
            type="group" text="nitrates"/>
