        <ddi id="DDI-DrugBank.d48.s12.d1" e1="DDI-DrugBank.d48.s12.e1"
            e2="DDI-DrugBank.d48.s12.e2" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d48.s13" text="The use of antacids should be considered in place of H2 blockers or proton pump inhibitors in patients receiving SPRYCEL therapy.">
        <entity id="DDI-DrugBank.d48.s13.e0" charOffset="11-18"
            type="group" text="antacids"/>
        <entity id="DDI-DrugBank.d48.s13.e1" charOffset="53-63"
            type="group" text="H2 blockers"/>
        <entity id="DDI-DrugBank.d48.s13.e2" charOffset="68-89"
            type="group" text="proton pump inhibitors"/>
        <entity id="DDI-DrugBank.d48.s13.e3" charOffset="113-119"
