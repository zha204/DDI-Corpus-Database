        <ddi id="DDI-DrugBank.d330.s5.d0" e1="DDI-DrugBank.d330.s5.e0"
            e2="DDI-DrugBank.d330.s5.e1" type="effect"/>
        <ddi id="DDI-DrugBank.d330.s5.d1" e1="DDI-DrugBank.d330.s5.e0"
