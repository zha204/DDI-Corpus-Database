    <sentence id="DDI-DrugBank.d60.s13" text="Resistance to the neuromuscular blocking action of nondepolarizing neuromuscular blocking agents has been demonstrated in patients chronically administered phenytoin or carbamazepine.">
        <entity id="DDI-DrugBank.d60.s13.e0" charOffset="51-95"
            type="group" text="nondepolarizing neuromuscular blocking agents"/>
        <entity id="DDI-DrugBank.d60.s13.e1" charOffset="156-164"
            type="drug" text="phenytoin"/>
        <entity id="DDI-DrugBank.d60.s13.e2" charOffset="169-181"
            type="drug" text="carbamazepine"/>
