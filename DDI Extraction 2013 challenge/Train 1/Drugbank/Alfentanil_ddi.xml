    <sentence id="DDI-DrugBank.d8.s4" text="Cimetidine reduces the clearance of ALFENTA.">
        <entity id="DDI-DrugBank.d8.s4.e0" charOffset="0-9" type="drug" text="Cimetidine"/>
        <entity id="DDI-DrugBank.d8.s4.e1" charOffset="36-42"
