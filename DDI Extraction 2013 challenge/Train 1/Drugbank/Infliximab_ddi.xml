    </sentence>
    <sentence id="DDI-DrugBank.d45.s7" text="Patients with Crohn s disease who received immunosuppressants tended to experience fewer infusion reactions compared to patients on no immunosuppressants.">
        <entity id="DDI-DrugBank.d45.s7.e0" charOffset="43-60"
            type="group" text="immunosuppressants"/>
