            type="group" text="estrogens"/>
        <entity id="DDI-DrugBank.d225.s6.e6" charOffset="122-135"
            type="group" text="contraceptives"/>
        <entity id="DDI-DrugBank.d225.s6.e7" charOffset="138-146"
            type="drug" text="phenytoin"/>
        <entity id="DDI-DrugBank.d225.s6.e8" charOffset="149-162"
