    <sentence id="DDI-DrugBank.d456.s0" text="Co-treatment with the potent CYP3A4 inhibitor ketoconazole increases erlotinib AUC by 2/3.">
        <entity id="DDI-DrugBank.d456.s0.e0" charOffset="46-57"
            type="drug" text="ketoconazole"/>
        <entity id="DDI-DrugBank.d456.s0.e1" charOffset="69-77"
            type="drug" text="erlotinib"/>
