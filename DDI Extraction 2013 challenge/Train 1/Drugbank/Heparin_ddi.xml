    <sentence id="DDI-DrugBank.d488.s3" text="The anticoagulant effect of heparin is enhanced by concurrent treatment with antithrombin III (human) in patients with hereditary antithrombin III deficiency.">
        <entity id="DDI-DrugBank.d488.s3.e0" charOffset="28-34"
            type="drug" text="heparin"/>
        <entity id="DDI-DrugBank.d488.s3.e1" charOffset="77-92"
            type="drug" text="antithrombin III"/>
        <ddi id="DDI-DrugBank.d488.s3.d0" e1="DDI-DrugBank.d488.s3.e0"
            e2="DDI-DrugBank.d488.s3.e1" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d488.s4" text="Thus in order to avoid bleeding, reduced dosage of heparin is recommended during treatment with antithrombin III (human).">
        <entity id="DDI-DrugBank.d488.s4.e0" charOffset="51-57"
