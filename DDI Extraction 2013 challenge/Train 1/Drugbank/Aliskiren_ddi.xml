    <sentence id="DDI-DrugBank.d533.s0" text="Effects of Other Drugs on Aliskiren Based on in-vitro studies, aliskiren is metabolized by CYP 3A4.">
        <entity id="DDI-DrugBank.d533.s0.e0" charOffset="26-34"
            type="drug" text="Aliskiren"/>
        <entity id="DDI-DrugBank.d533.s0.e1" charOffset="63-71"
            type="drug" text="aliskiren"/>
    </sentence>
