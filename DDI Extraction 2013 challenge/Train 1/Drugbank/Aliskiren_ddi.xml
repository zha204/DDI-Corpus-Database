    <sentence id="DDI-DrugBank.d533.s2" text="Co-administration of irbesartan reduced aliskiren Cmax up to 50% after multiple dosing.">
        <entity id="DDI-DrugBank.d533.s2.e0" charOffset="21-30"
            type="drug" text="irbesartan"/>
        <entity id="DDI-DrugBank.d533.s2.e1" charOffset="40-48"
            type="drug" text="aliskiren"/>
