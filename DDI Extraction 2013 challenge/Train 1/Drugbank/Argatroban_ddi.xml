    <sentence id="DDI-DrugBank.d148.s3" text="Oral anticoagulant agents: Pharmacokinetic drug-drug interactions between Argatroban and warfarin (7.5 mg single oral dose) have not been demonstrated.">
        <entity id="DDI-DrugBank.d148.s3.e0" charOffset="5-24"
            type="group" text="anticoagulant agents"/>
        <entity id="DDI-DrugBank.d148.s3.e1" charOffset="74-83"
