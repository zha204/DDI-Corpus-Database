            type="drug" text="ketoconazole"/>
        <entity id="DDI-DrugBank.d89.s3.e4" charOffset="194-201"
            type="drug" text="caffeine"/>
        <entity id="DDI-DrugBank.d89.s3.e5" charOffset="273-280"
            type="drug" text="caffeine"/>
        <entity id="DDI-DrugBank.d89.s3.e6" charOffset="301-313"
            type="drug" text="phenobarbital"/>
        <entity id="DDI-DrugBank.d89.s3.e7" charOffset="319-327"
            type="drug" text="phenytoin"/>
        <ddi id="DDI-DrugBank.d89.s3.d0" e1="DDI-DrugBank.d89.s3.e0"
