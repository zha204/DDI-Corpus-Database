        <entity id="DDI-DrugBank.d78.s3.e2" charOffset="21-33"
            type="drug" text="Grepafloxacin"/>
        <entity id="DDI-DrugBank.d78.s3.e3" charOffset="47-56"
            type="group" text="quinolones"/>
        <entity id="DDI-DrugBank.d78.s3.e4" charOffset="89-96"
