        <ddi id="DDI-DrugBank.d72.s0.d0" e1="DDI-DrugBank.d72.s0.e0"
            e2="DDI-DrugBank.d72.s0.e1" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d72.s1" text="Fluconazole, and the 5-HT3 antiemetics ondansetron (Zofran) and granisetron (Kytril) have all been used with BUSULFEX.">
        <entity id="DDI-DrugBank.d72.s1.e0" charOffset="0-10"
            type="drug" text="Fluconazole"/>
        <entity id="DDI-DrugBank.d72.s1.e1" charOffset="21-37"
