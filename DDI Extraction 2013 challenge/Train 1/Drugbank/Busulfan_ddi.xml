    <sentence id="DDI-DrugBank.d72.s0" text="Itraconazole decreases busulfan clearance by up to 25%, and may produce AUCs   1500  M min in some patients.">
        <entity id="DDI-DrugBank.d72.s0.e0" charOffset="0-11"
            type="drug" text="Itraconazole"/>
        <entity id="DDI-DrugBank.d72.s0.e1" charOffset="23-30"
