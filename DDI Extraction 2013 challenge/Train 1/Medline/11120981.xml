<document id="DDI-MedLine.d79">
    <sentence id="DDI-MedLine.d79.s0" text="If taken 1 hour before indinavir (IDV), didanosine does not affect IDV exposure, despite persistent buffering effects.&#xd;&#xa;">
        <entity id="DDI-MedLine.d79.s0.e0" charOffset="23-31"
            type="drug" text="indinavir"/>
        <entity id="DDI-MedLine.d79.s0.e1" charOffset="34-36"
            type="drug" text="IDV"/>
