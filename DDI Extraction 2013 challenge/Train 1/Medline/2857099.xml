<document id="DDI-MedLine.d27">
    <sentence id="DDI-MedLine.d27.s0" text="[The GABA-ergic system and brain edema]&#xd;&#xa;It has been shown in rats with experimental toxic and traumatic edemas that picrotoxin (1 mg/kg) removes the antiedematous action of diazepam, phenazepam, phenibut and amizyl and reduces the action of phentolamine. ">
        <entity id="DDI-MedLine.d27.s0.e0" charOffset="117-126"
            type="drug_n" text="picrotoxin"/>
        <entity id="DDI-MedLine.d27.s0.e1" charOffset="174-181"
            type="drug" text="diazepam"/>
        <entity id="DDI-MedLine.d27.s0.e2" charOffset="184-193"
            type="drug_n" text="phenazepam"/>
        <entity id="DDI-MedLine.d27.s0.e3" charOffset="196-203"
            type="drug_n" text="phenibut"/>
        <entity id="DDI-MedLine.d27.s0.e4" charOffset="209-214"
            type="drug_n" text="amizyl"/>
        <entity id="DDI-MedLine.d27.s0.e5" charOffset="242-253"
            type="drug" text="phentolamine"/>
        <ddi id="DDI-MedLine.d27.s0.d0" e1="DDI-MedLine.d27.s0.e0"
            e2="DDI-MedLine.d27.s0.e1" type="effect"/>
        <ddi id="DDI-MedLine.d27.s0.d1" e1="DDI-MedLine.d27.s0.e0"
            e2="DDI-MedLine.d27.s0.e2" type="effect"/>
        <ddi id="DDI-MedLine.d27.s0.d2" e1="DDI-MedLine.d27.s0.e0"
            e2="DDI-MedLine.d27.s0.e3" type="effect"/>
        <ddi id="DDI-MedLine.d27.s0.d3" e1="DDI-MedLine.d27.s0.e0"
            e2="DDI-MedLine.d27.s0.e4" type="effect"/>
        <ddi id="DDI-MedLine.d27.s0.d4" e1="DDI-MedLine.d27.s0.e0"
            e2="DDI-MedLine.d27.s0.e5" type="effect"/>
    </sentence>
    <sentence id="DDI-MedLine.d27.s1" text="When the dose of picrotoxin is minimized to 0.5 mg/kg such an effect is not observed. ">
        <entity id="DDI-MedLine.d27.s1.e0" charOffset="17-26"
            type="drug_n" text="picrotoxin"/>
    </sentence>
    <sentence id="DDI-MedLine.d27.s2" text="Prolonged daily administration of picrotoxin in a dose of 1 mg/kg results in the development of brain edema. ">
        <entity id="DDI-MedLine.d27.s2.e0" charOffset="34-43"
            type="drug_n" text="picrotoxin"/>
    </sentence>
    <sentence id="DDI-MedLine.d27.s3" text="It is recommended that GABA-positive drugs be included into a complex of treatment measures for edema."/>
</document>
